<%= form_for @articulo do |form| %>
  <div>
    <%= form.label :descripcion %><br>
    <%= form.text_field :descripcion %>
    <% @articulo.errors.full_messages_for(:descripcion).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>